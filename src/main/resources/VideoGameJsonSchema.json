{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": [
    "id",
    "name",
    "releaseDate",
    "reviewScore",
    "category",
    "rating"
  ],
  "properties": {
    "id": {
      "type": "integer",
      "default": 0,
      "title": "The id Schema",
      "examples": [
        1
      ]
    },
    "name": {
      "type": "string",
      "default": "",
      "title": "The name Schema",
      "examples": [
        "Resident Evil 4"
      ]
    },
    "releaseDate": {
      "type": "string",
      "default": "",
      "title": "The releaseDate Schema",
      "examples": [
        "2005-10-01"
      ]
    },
    "reviewScore": {
      "type": "integer",
      "default": 0,
      "title": "The reviewScore Schema",
      "examples": [
        85
      ]
    },
    "category": {
      "type": "string",
      "default": "",
      "title": "The category Schema",
      "examples": [
        "Shooter"
      ]
    },
    "rating": {
      "type": "string",
      "default": "",
      "title": "The rating Schema",
      "examples": [
        "Universal"
      ]
    }
  },
  "examples": [{
    "id": 1,
    "name": "Resident Evil 4",
    "releaseDate": "2005-10-01",
    "reviewScore": 85,
    "category": "Shooter",
    "rating": "Universal"
  }]
}